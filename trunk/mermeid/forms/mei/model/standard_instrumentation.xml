        <instrVoice count="2" code="bb">tr.</instrVoice>
        <instrVoice count="3." code="bd">trb.</instrVoice>
        <instrVoice count="1" code="be">tb.</instrVoice>
        <instrVoice count="1" code="pa">timp.</instrVoice>

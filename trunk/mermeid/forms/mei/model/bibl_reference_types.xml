<?xml version="1.0" encoding="UTF-8"?>
<TEI xmlns="http://www.tei-c.org/ns/1.0">
  <!-- Bibliographic reference type definitions -->
  <!-- DCM 2012 -->
  

  <!-- special reference types for primary texts-->
  <listBibl type="primary">
    
    <bibl type="Letter" xml:id="">
      <author/>
      <name role="recipient"/>
      <date/>
      <geogName/>
      <msIdentifier>
        <repository/>
        <idno/>
      </msIdentifier>
      <note/>
      <ref target=""/>
      <ref type="editions">
        <bibl>
          <title type="short_title"/>
          <biblScope/>
        </bibl>
      </ref>
    </bibl>
    
    <bibl type="Diary_entry" xml:id="">
      <author/>
      <date/>
      <geogName/>
      <msIdentifier>
        <repository/>
        <idno/>
      </msIdentifier>
      <note/>

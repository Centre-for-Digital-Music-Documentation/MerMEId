                    <title xml:id="title_idd68da4b7" xml:lang="da">[Song cycle:] Seks Sange til Tekster af Ludvig Holstein</title>
                    <title xml:id="title_e5bfb1b5" xml:lang="en">Six Songs to Texts by Ludvig Holstein</title>
                    <title type="text_source" xml:id="title_1901cdac" xml:lang="da">Digte. København 1895</title>
                    <respStmt>
                        <persName role="composer" xml:id="persName_idfd4e4845">Carl Nielsen</persName>

<?xml version="1.0" encoding="UTF-8"?>
<perfResList xmlns="http://www.music-encoding.org/ns/mei">
    <!-- Pre-defined instrument groups for easy inserting. -->
    
    <!-- First, a few empty (undefined) instrument groups. Please do not change. -->
    <perfResList codedval="on">
        <head/>
    </perfResList>
    <perfResList>
        <head/>
        <perfRes count="1" codedval=""></perfRes>
    </perfResList>
    
    <!-- Customizable part starts here -->

    <!-- Pre-defined ensembles. The <perfResList> must have @code value to be recognized as an ensemble. -->
    <!-- The @code value does not need to be unique, however. 
    For instance, several orchestras of different sizes may be defined, all having @code value "oa". -->
    <!-- @code values refer to MARC:048 ensembles as defined at http://www.loc.gov/marc/bibliographic/bd048.html -->
    <perfResList codedval="oa">
        <head>Orchestra 2.2.2.2/4.2.3.1/timp./str.</head>
        <perfRes count="2" codedval="wa">fl.</perfRes>
        <perfRes count="2" codedval="wb">ob.</perfRes>
        <perfRes count="2" codedval="wc">cl.</perfRes>
        <perfRes count="2" codedval="wd">fg.</perfRes>
        <perfRes count="4" codedval="ba">cor.</perfRes>
        <perfRes count="2" codedval="bb">tr.</perfRes>
        <perfRes count="3" codedval="bd">trb.</perfRes>
        <perfRes count="1" codedval="be">tb.</perfRes>
        <perfRes count="1" codedval="pa">timp.</perfRes>
        <perfRes count="" codedval="sn">str.</perfRes>
    </perfResList>
    <perfResList codedval="oc">
        <head>Strings (vl.1,2, va., vc., cb.)</head>
        <perfRes count="" codedval="sa">vl.1</perfRes>
        <perfRes count="" codedval="sa">vl.2</perfRes>
        <perfRes count="" codedval="sb">va.</perfRes>
        <perfRes count="" codedval="sc">vc.</perfRes>
        <perfRes count="" codedval="sd">cb.</perfRes>
    </perfResList>
    <perfResList codedval="oc">
        <head>String quartet</head>
        <perfRes count="2" codedval="sa">vl.</perfRes>
        <perfRes count="1" codedval="sb">va.</perfRes>
        <perfRes count="1" codedval="sc">vc.</perfRes>
    </perfResList>
    <perfResList codedval="ca">
        <head>Choir SATB</head>
        <perfRes count="1" codedval="va">S.</perfRes>
        <perfRes count="1" codedval="vc">A.</perfRes>
        <perfRes count="1" codedval="vd">T.</perfRes>
        <perfRes count="1" codedval="vf">B.</perfRes>
    </perfResList>
    <perfResList codedval="ca">
        <head>Choir SSAATTBB</head>
        <perfRes count="2" codedval="va">S.</perfRes>
        <perfRes count="2" codedval="vc">A.</perfRes>
        <perfRes count="2" codedval="vd">T.</perfRes>
        <perfRes count="2" codedval="vf">B.</perfRes>
    </perfResList>
    <perfResList codedval="cc">
        <head>Men's choir</head>
        <perfRes count="1" codedval="vd">T.</perfRes>
        <perfRes count="1" codedval="vd">T.</perfRes>
        <perfRes count="1" codedval="vf">B.</perfRes>
        <perfRes count="1" codedval="vf">B.</perfRes>
    </perfResList>
    
    <!-- Groups without a "code" attribute are not shown in the 
        list of pre-defined entire ensembles; instead, MerMEId uses them to insert a number of 
        individual instruments at "performer" level, i.e. as part of an 
        orchestra or as non-ensemble performers. -->

    <perfResList>
        <head>Woodwinds 2.2.2.2</head>
        <perfRes count="2" codedval="wa">fl.</perfRes>
        <perfRes count="2" codedval="wb">ob.</perfRes>
        <perfRes count="2" codedval="wc">cl.</perfRes>
        <perfRes count="2" codedval="wd">fg.</perfRes>
    </perfResList>
    <perfResList>
        <head>Brass 4.2.3.1 + timp.</head>
        <perfRes count="4" codedval="ba">cor.</perfRes>
        <perfRes count="2" codedval="bb">tr.</perfRes>
        <perfRes count="3" codedval="bd">trb.</perfRes>
        <perfRes count="1" codedval="be">tb.</perfRes>
        <perfRes count="1" codedval="pa">timp.</perfRes>
    </perfResList>
    <perfResList>
        <head>Strings (unspec.)</head>
        <perfRes count="" codedval="sn">str.</perfRes>
    </perfResList>
    <perfResList>
        <head>Voice &amp; piano</head>
        <perfRes count="1" codedval="vn">voice</perfRes>
        <perfRes count="1" codedval="ka">pno.</perfRes>
    </perfResList>
    
</perfResList>

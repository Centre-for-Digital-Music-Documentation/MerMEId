        <perfRes authority="MARC" authURI="https://www.loc.gov/standards/valuelist/marcmusperf.html" count="1" codedval="vf" xml:lang="it">B.</perfRes>
    </perfResList>
    
    <!-- Groups without a "code" attribute are not shown in the 
        list of pre-defined entire ensembles; instead, MerMEId uses them to insert a number of 
        individual instruments at "performer" level, i.e. as part of an 
        orchestra or as non-ensemble performers. -->


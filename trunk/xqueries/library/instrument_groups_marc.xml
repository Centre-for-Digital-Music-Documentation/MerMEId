        <perfRes authority="MARC" authURI="https://www.loc.gov/standards/valuelist/marcmusperf.html" count="2" codedval="wb" xml:lang="it">ob.</perfRes>
        <perfRes authority="MARC" authURI="https://www.loc.gov/standards/valuelist/marcmusperf.html" count="2" codedval="wc" xml:lang="it">cl.</perfRes>
        <perfRes authority="MARC" authURI="https://www.loc.gov/standards/valuelist/marcmusperf.html" count="2" codedval="wd" xml:lang="it">fg.</perfRes>

<?xml version="1.0" encoding="UTF-8"?>
<perfResList authority="MARC" authURI="https://www.loc.gov/standards/valuelist/marcmusperf.html" xmlns="http://www.music-encoding.org/ns/mei">
    <!-- Pre-defined instrument groups for easy inserting. -->
    <!-- @codedval values refer to MARC:048 ensembles as defined at http://www.loc.gov/marc/bibliographic/bd048.html -->
    
    <!-- First, a few empty (undefined) instrument groups. Please do not change. -->
    <perfResList authority="MARC" authURI="https://www.loc.gov/standards/valuelist/marcmusperf.html" codedval="on">
        <head/>
    </perfResList>
    <perfResList>
        <head/>
        <perfRes count="1" authority="MARC" authURI="https://www.loc.gov/standards/valuelist/marcmusperf.html" codedval=""></perfRes>
    </perfResList>
    
    <!-- Customizable part starts here -->

    <!-- Pre-defined ensembles. The <perfResList> must have @code value to be recognized as an ensemble. -->
    <!-- The @code value does not need to be unique, however. For instance, several orchestras of different sizes may be defined, all having @codedval="oa". -->
    <!-- @label values are used for the editor menu only; MerMEId does not include them in the encoding -->    
    
    <perfResList authority="MARC" authURI="https://www.loc.gov/standards/valuelist/marcmusperf.html" codedval="oa" label="Orchestra 2.2.2.2/4.2.3.1/timp./str.">
        <head>Orchestra</head>

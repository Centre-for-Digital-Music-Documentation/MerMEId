        <perfRes authority="MARC" authURI="https://www.loc.gov/standards/valuelist/marcmusperf.html" count="1" codedval="pa" xml:lang="it">timp.</perfRes>
        <perfRes authority="MARC" authURI="https://www.loc.gov/standards/valuelist/marcmusperf.html" count="" codedval="sn" xml:lang="en">str.</perfRes>
    </perfResList>

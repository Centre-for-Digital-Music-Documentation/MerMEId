                    <title xml:id="title_idd68da4b7" xml:lang="da">Seks Sange til Tekster af Ludvig Holstein</title>
                    <title xml:id="title_e5bfb1b5" xml:lang="en">Six Songs to Texts by Ludvig Holstein</title>
                    <respStmt>
                        <persName role="composer" xml:id="persName_idfd4e4845">Carl Nielsen</persName>
                        <persName role="author" xml:id="persName_a3f3f660">Ludvig Holstein</persName>
